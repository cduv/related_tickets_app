<div>
	{{#if results}}
		<table class="table table-condensed">
			<tbody>
				{{#each results}}
					<tr class="_tooltip" data-placement="top">
						<td>#{{id}}</td>
						<td><a href="/agent/#/tickets/{{id}}" target="_blank">{{subject}}</a></td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	{{else}}
		<p>{{t "global.no_results"}}</p>
	{{/if}}
</div>